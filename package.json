{
  "name": "humex-champions",
  "private": true,
  "version": "0.22.0",
  "type": "module",
  "scripts": {
    "dev:vite": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --ext .ts,.tsx --fix src/",
    "preview": "vite preview",
    "format": "prettier --write ./src",
    "release": "standard-version",
    "gen-lang-types": "i18next-resources-for-ts interface -i ./src/locales -o ./src/types/resources.d.ts",
    "watch-lang": "nodemon --watch src/locales/en.json --exec \"npm run gen-lang-types\"",
    "dev": "npm-run-all --parallel dev:vite watch-lang"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^5.16.4",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.4",
    "@mui/x-charts": "^7.11.1",
    "@mui/x-data-grid": "^7.21.0",
    "@mui/x-date-pickers": "^7.12.0",
    "dayjs": "^1.11.12",
    "firebase": "^10.12.4",
    "i18next": "^23.16.2",
    "i18next-resources-for-ts": "^1.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-i18next": "^15.1.0",
    "react-imask": "^7.6.1",
    "react-router-dom": "^6.25.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "3.3.3",
    "standard-version": "^9.5.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.10"
  }
}
